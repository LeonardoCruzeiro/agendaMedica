    private void jButtonSalvarUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                     
        
        if(jTextFieldNomeUsuario.getText().isEmpty()){
            JOptionPane.showMessageDialog(null,"Informer o nome de usuario");
        }else if(jPasswordFieldSenhaUsuario.getText().isEmpty()){
            JOptionPane.showMessageDialog(null,"Informer a senha");
        }else if(jPasswordFieldConfirmaSenhaUsuario.getText().isEmpty()){
            JOptionPane.showMessageDialog(null,"Informer o confirma senha");
        }else
        if(jPasswordFieldSenhaUsuario.getText().equals(jPasswordFieldConfirmaSenhaUsuario.getText())){
        if(flag==1){
        usu.setUsuNome(jTextFieldNomeUsuario.getText());
        usu.setUsuSenha(jPasswordFieldSenhaUsuario.getText());
        usu.setUsuTipo((String)jComboBoxTipoUsuario.getSelectedItem());
        usudao.Salvar(usu);
        
        jTextFieldNomeUsuario.setText("");
        jPasswordFieldSenhaUsuario.setText("");
        jPasswordFieldConfirmaSenhaUsuario.setText("");
        
        jTextFieldCodigoUsuario.setEnabled(false);
        jTextFieldNomeUsuario.setEnabled(false);
        jPasswordFieldSenhaUsuario.setEnabled(false);
        jPasswordFieldConfirmaSenhaUsuario.setEnabled(false);
        jComboBoxTipoUsuario.setEnabled(false);
        jButtonSalvarUsuario.setEnabled(false);
        preencherTabela("select * from usuario order by c_nomeusuario"); 
        }else{
        preencherTabela("select * from usuario order by c_nomeusuario");
        usu.setUsuCod((Integer.parseInt(jTextFieldCodigoUsuario.getText())));
        usu.setUsuNome(jTextFieldNomeUsuario.getText());
        usu.setUsuSenha(jPasswordFieldSenhaUsuario.getText());
        usu.setUsuTipo((String)jComboBoxTipoUsuario.getSelectedItem());
        usudao.Alterar(usu);
        jTextFieldCodigoUsuario.setText("");
        jTextFieldNomeUsuario.setText("");
        jPasswordFieldSenhaUsuario.setText("");
        jPasswordFieldConfirmaSenhaUsuario.setText("");
        jTextFieldPesquisarUsuario.setText("");
        
        jButtonNovoUsuario.setEnabled(true);
        jButtonExcluirUsuario.setEnabled(false);
        jButtonAlterarUsuario.setEnabled(false);
        jButtonSalvarUsuario.setEnabled(false);
        preencherTabela("select * from usuario order by c_nomeusuario");
        }
        }else{
            JOptionPane.showMessageDialog(null, "As senhas nao iguais");
        }
            
    }                                                    

    private void jButtonNovoUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                   
        flag=1;
        jTextFieldCodigoUsuario.setEnabled(true);
        jTextFieldNomeUsuario.setEnabled(true);
        jPasswordFieldSenhaUsuario.setEnabled(true);
        jPasswordFieldConfirmaSenhaUsuario.setEnabled(true);
        jComboBoxTipoUsuario.setEnabled(true);
        jTextFieldPesquisarUsuario.setEnabled(false);
        jTextFieldCodigoUsuario.setText("");
        jTextFieldNomeUsuario.setText("");
        jPasswordFieldSenhaUsuario.setText("");
        jPasswordFieldConfirmaSenhaUsuario.setText("");
        jTextFieldPesquisarUsuario.setText("");
        
        jButtonSalvarUsuario.setEnabled(true);
        jButtonNovoUsuario.setEnabled(true);
        jButtonCancelarUsuario.setEnabled(true);
        
        preencherTabela("select * from usuario order by c_nomeusuario");
    }                                                  

    private void jButtonBuscarUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                     
       usu.setPesquisar(jTextFieldPesquisarUsuario.getText());
       
       BeansUsuario model = usudao.buscaUsuario(usu);
       jTextFieldCodigoUsuario.setText(String.valueOf(model.getUsuCod()));
       jTextFieldNomeUsuario.setText(model.getUsuNome());
       jPasswordFieldSenhaUsuario.setText(model.getUsuSenha());
       jPasswordFieldConfirmaSenhaUsuario.setText(model.getUsuSenha());
       jComboBoxTipoUsuario.setSelectedItem(model.getUsuTipo());
       jButtonAlterarUsuario.setEnabled(!true);
       jButtonExcluirUsuario.setEnabled(!true);
    
      
       
     preencherTabela("select * from usuario where c_nomeusuario like'%"+model.getPesquisar()+"%'");  
    }                                                    

    private void jButtonExcluirUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                      
        int resposta = 0;
        resposta= JOptionPane.showConfirmDialog(rootPane,"Deseja realmente excluir ? ");
        if (resposta == JOptionPane.YES_NO_OPTION){
            usu.setUsuCod(Integer.parseInt(jTextFieldCodigoUsuario.getText()));
            usudao.Excluir(usu);
            
        jTextFieldCodigoUsuario.setText("");
        jTextFieldNomeUsuario.setText("");
        jPasswordFieldSenhaUsuario.setText("");
        jPasswordFieldConfirmaSenhaUsuario.setText("");
        jTextFieldPesquisarUsuario.setText("");  
          preencherTabela("select * from usuario order by c_nomeusuario"); 
        }
        jButtonExcluirUsuario.setEnabled(false);
        jButtonAlterarUsuario.setEnabled(false);
        jButtonCancelarUsuario.setEnabled(false);
        jButtonNovoUsuario.setEnabled(true);
        jButtonSalvarUsuario.setEnabled(false);
    }                                                     

    private void jButtonAlterarUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                      
        flag = 2;
        jTextFieldNomeUsuario.setEnabled(true);
        jPasswordFieldSenhaUsuario.setEnabled(true);
        jPasswordFieldConfirmaSenhaUsuario.setEnabled(true);
        jComboBoxTipoUsuario.setEnabled(true);
        
        
        jButtonNovoUsuario.setEnabled(false);
        jButtonSalvarUsuario.setEnabled(true);
        
        preencherTabela("select * from usuario order by c_nomeusuario");
    }                                                     

    private void jButtonCancelarUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                       
        jTextFieldCodigoUsuario.setEnabled(!true);
        jTextFieldNomeUsuario.setEnabled(!true);
        jPasswordFieldSenhaUsuario.setEnabled(!true);
        jPasswordFieldConfirmaSenhaUsuario.setEnabled(!true);
        jComboBoxTipoUsuario.setEnabled(!true);
        jTextFieldPesquisarUsuario.setEnabled(true);
        jTextFieldCodigoUsuario.setText("");
        jTextFieldNomeUsuario.setText("");
        jPasswordFieldSenhaUsuario.setText("");
        jPasswordFieldConfirmaSenhaUsuario.setText("");
        jTextFieldPesquisarUsuario.setText("");
        
        jButtonSalvarUsuario.setEnabled(!true);
        jButtonNovoUsuario.setEnabled(true);
        jButtonCancelarUsuario.setEnabled(false);
    }                                                      

    private void jTableUsuarioMouseClicked(java.awt.event.MouseEvent evt) {                                           
        String nome_usuario = ""+jTableUsuario.getValueAt(jTableUsuario.getSelectedRow(),1);
        conex.conexao();
        conex.executaSQL("select * from usuario where c_nomeusuario='"+nome_usuario+"'");
        try{
            conex.rs.first();
            jTextFieldCodigoUsuario.setText(String.valueOf(conex.rs.getInt("n_numeusuario")));
            jTextFieldNomeUsuario.setText(conex.rs.getString("c_nomeusuario"));
            jPasswordFieldSenhaUsuario.setText(conex.rs.getString("n_senhausuario"));
            jPasswordFieldConfirmaSenhaUsuario.setText(conex.rs.getString("n_senhausuario"));
            jComboBoxTipoUsuario.setSelectedItem(conex.rs.getString("c_tipousuario"));
        }catch(SQLException ex){
            JOptionPane.showMessageDialog(null,"" +ex);
        }
        conex.desconecta();
        jTextFieldCodigoUsuario.setEnabled(false);
        jButtonSalvarUsuario.setEnabled(false);
        jButtonExcluirUsuario.setEnabled(true);
        jButtonAlterarUsuario.setEnabled(true);
        jButtonNovoUsuario.setEnabled(true);
        preencherTabela("select * from usuario order by c_nomeusuario");
    }                                          

    private void jTextFieldCodigoUsuarioActionPerformed(java.awt.event.ActionEvent evt) {                                                        
       
    }                                                       
    
    public void preencherTabela(String Sql){
        ArrayList dados = new ArrayList();
        String [] coluna = new String[]{"codigo","nome","tipo"};
        
        conex.conexao();
        conex.executaSQL(Sql);
        
        try{
            conex.rs.first();
            do{
                dados.add(new Object[]{conex.rs.getInt("n_numeusuario"),conex.rs.getString("c_nomeusuario"),conex.rs.getString("c_tipousuario")});
                
              }while(conex.rs.next());
                        
        }catch(SQLException ex){
            JOptionPane.showMessageDialog(null, "erro ao preencher arraylist"+ex);
        }
        
        ModeloTabela modelo = new ModeloTabela(dados,coluna);
        
        jTableUsuario.setModel(modelo);
        jTableUsuario.getColumnModel().getColumn(0).setPreferredWidth(50);
        jTableUsuario.getColumnModel().getColumn(0).setResizable(false);
        jTableUsuario.getColumnModel().getColumn(1).setPreferredWidth(220);
        jTableUsuario.getColumnModel().getColumn(1).setResizable(false);
        jTableUsuario.getColumnModel().getColumn(2).setPreferredWidth(100);
        jTableUsuario.getColumnModel().getColumn(2).setResizable(false);
       
        
        
        jTableUsuario.getTableHeader().setReorderingAllowed(false);
        jTableUsuario.setAutoResizeMode(jTableUsuario.AUTO_RESIZE_OFF);
        jTableUsuario.setSelectionMode(ListSelectionModel.SINGLE_SELECTION);
        
        
        conex.desconecta();
        
    }
    
